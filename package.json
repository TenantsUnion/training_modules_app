{
  "name": "tu_training_modules",
  "version": "0.0.1-SNAPSHOT",
  "description": "Tenants Union training modules application",
  "main": "app.js",
  "scripts": {
    "start": "set NODE_CONFIG_DIR=./config && ./bin/start",
    "init_db": "set NODE_CONFIG_DIR=./config && node $NODE_DEBUG_OPTION ./bin/init_db",
    "drop_db": "set NODE_CONFIG_DIR=./config && node $NODE_DEBUG_OPTION ./bin/drop_db",
    "build": "npm run server-build && npm run webpack-build",
    "migrate_db": "set NODE_CONFIG_DIR=./config && ./bin/migrate_db",
    "webpack-build": "webpack --config webapp/webpack.config.js",
    "webpack-dev": "webpack-dev-server --config webapp/webpack.config.js --open",
    "server-build": "tsc --project server/tsconfig.json --outDir server/dist",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Server-Build,Server,Webpack\" -c \"cyan.bold,green.bold,yellow.bold\"  \"npm run watch-ts\" \"npm run nodemon-start\" \"npm run webpack-dev\"",
    "watch-ts": "tsc --watch --project server/tsconfig.json --outDir server/dist",
    "nodemon-start": "set NODE_CONFIG_DIR=./config && nodemon --watch server/dist/ ./bin/start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/egarasky/tu_training_modules.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/egarasky/tu_training_modules#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "config": "^1.26.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-engine": "^1.0.1",
    "foundation-sites": "^6.4.1",
    "hbs": "~4.0.1",
    "morgan": "~1.8.1",
    "pg": "^7.0.2",
    "q": "^1.5.0",
    "quill": "^1.3.0",
    "serve-favicon": "~2.4.2",
    "underscore": "^1.8.3",
    "vue": "^2.3.4",
    "vue-router": "^2.7.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.12",
    "@types/quill": "0.0.31",
    "@types/underscore": "^1.8.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "loglevel": "^1.4.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.6",
    "postgrator": "^2.10.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.0",
    "ts-loader": "^2.3.0",
    "typescript": "^2.4.1",
    "vue-class-component": "^5.0.2",
    "vue-loader": "^12.2.2",
    "vue-template-compiler": "^2.4.1",
    "vue-ts-loader": "0.0.3",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.0"
  }
}
