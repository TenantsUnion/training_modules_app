{
  "name": "tu_training_modules",
  "version": "0.0.1-SNAPSHOT",
  "description": "Tenants Union training modules application",
  "main": "app.js",
  "scripts": {
    "start": "set NODE_CONFIG_DIR=./config && ./bin/start",
    "build": "npm run build-server && npm run build-webapp",
    "build-webpack": "webpack --config webapp/webpack.config.js",
    "build-server": "tsc --project server/tsconfig.json --outDir server/dist",
    "build-webapp": "gulp build --gulpfile webapp/gulpfile.js",
    "watch-webapp": "gulp watch --gulpfile webapp/gulpfile.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Server-Build,Server,Webapp-Build\" -c \"cyan.bold,green.bold,yellow.bold\"  \"npm run watch-ts\" \"npm run nodemon-start\" \"npm run watch-webapp\"",
    "watch-ts": "tsc --watch --project server/tsconfig.json --outDir server/dist",
    "nodemon-start": "set NODE_CONFIG_DIR=./config && nodemon --watch server/dist/ ./bin/start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/egarasky/tu_training_modules.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/egarasky/tu_training_modules#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "config": "^1.26.1",
    "cookie-parser": "~1.4.3",
    "db-migrate": "^0.10.0-beta.21",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-engine": "^1.0.1",
    "hbs": "~4.0.1",
    "morgan": "~1.8.1",
    "q": "^1.5.0",
    "serve-favicon": "~2.4.2",
    "underscore": "^1.8.3",
    "vue": "^2.3.4",
    "vue-quill-editor": "^2.2.4",
    "vue-router": "^2.7.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.12",
    "@types/underscore": "^1.8.1",
    "browser-sync": "^2.18.12",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "db-migrate": "^0.10.0-beta.21",
    "db-migrate-pg": "^0.2.4",
    "gulp-if": "^2.0.2",
    "gulp-nodemon": "^2.2.1",
