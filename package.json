    "@types/quill": "0.0.31",
    "@types/socket.io": "1.4.31",
    "@types/socket.io-client": "1.4.31",
    "@types/underscore": "1.8.3",
    "@types/winston": "2.3.5",
    "chai": "4.1.2",
    "chalk": "2.3.0",
    "concurrently": "3.5.0",
    "cross-env": "5.1.3",
    "css-loader": "0.28.7",
