{
  "name": "tu_training_modules",
  "version": "0.0.1-SNAPSHOT",
  "description": "Tenants Union training modules application",
  "main": "app.js",
  "scripts": {
    "start": "cross-env SCRIPT_NAME=start npm run esm-entry",
    "e2e-start": "cross-env NODE_ENV=test SCRIPT_NAME=start npm run esm-entry",
    "esm-entry": "cross-env NODE_CONFIG_DIR=./config node ./bin/esm_script_runner",
    "init-db": "cross-env SCRIPT_NAME=init_db npm run esm-entry && npm run migrate-db",
    "old-init-db": "export NODE_CONFIG_DIR=./config && node ./bin/drop-db && node ./bin/init_db && npm run migrate-db",
    "drop-db": "export SCRIPT_NAME=drop_db && npm run esm-entry",
    "migrate-db": "cross-env SCRIPT_NAME=migrate_db npm run esm-entry",
    "build": "npm run server-build && npm run webpack-build",
    "webpack-build": "cross-env NODE_ENV=production node webapp/webpack/build.js",
    "webpack-dev": "webpack-dev-server --config webapp/webpack/dev.webpack.config.js",
    "server-build": "tsc --project server/tsconfig.json --outDir server/dist",
    "watch-webapp": "",
    "watch-dev": "concurrently -k -p \"[{name}]\" -n \"Server-Build,Server,Webpack\" -c \"cyan.bold,green.bold,yellow.bold\"  \"npm run watch-build-server\" \"npm run nodemon-start\" \"npm run webpack-dev\"",
    "watch-server": "concurrently -k -p \"[{name}]\" -n \"Server-Build,Server\" -c \"cyan.bold,green.bold\"  \"npm run watch-build-server\" \"npm run nodemon-start\"",
    "watch-build-server": "tsc --pretty --watch --project server/tsconfig.json --outDir server/dist",
    "nodemon-start": "cross-env NODE_CONFIG_DIR=./config nodemon --watch server/dist/ --signal SIGHUP --delay 1500ms --exec npm run start",
    "test-watch": "cross-env NODE_ENV=test npm run watch",
    "test-task": "cross-env NODE_ENV=test npm run $TASK_NAME",
    "test-drop-db": "cross-env TASK_NAME=drop-db npm run test-task",
    "test-migrate-db": "cross-env TASK_NAME=migrate-db npm run test-task",
    "test-init-db": "cross-env SCRIPT_NAME=init_test_db TASK_NAME=esm-entry npm run test-task && npm run test-migrate-db",
    "test-command": "cross-env NODE_ENV=test $COMMAND",
    "test-mocha": "cross-env COMMAND='mocha --opts mocha_unit_test/mocha.opts mocha_unit_test/**/*.spec.ts' npm run test-command",
    "test-karma": "cross-env COMMAND='karma start webapp/karma_unit_test/karma.unit.js --singleRun' npm run test-command",
    "test-server": "npm run test-init-db && npm run test-mocha",
    "test-server-dev": "cross-env COMMAND='mocha mocha_unit_test/**/*.spec.ts --opts mocha_unit_test/mocha.opts -R min --watch --watch-extensions ts' npm run test-command",
    "test-karma-dev": "cross-env COMMAND='karma start webapp/karma_unit_test/karma.unit.js' npm run test-command",
    "test-e2e": "cross-env COMMAND='ts-node testcafe_e2e_test/build_runner.ts -P testcafe_e2e_test/tsconfig.json' npm run test-command",
    "test": "npm run test-server && npm run test-karma",
    "rm-ide-compiled-ts": "./bin/rm_ide_compiled_ts.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/egarasky/tu_training_modules.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/egarasky/tu_training_modules#readme",
  "@std/esm": {
    "esm": "js",
    "await": true,
    "cjs": true
  },
  "_moduleAliases": {
    "@shared": "./server/dist/shared",
    "@course": "./server/dist/server/src/course",
    "@module": "./server/dist/server/src/module",
    "@section": "./server/dist/server/src/section",
    "@util": "./server/dist/server/src/util"
  },
  "dependencies": {
    "@std/esm": "0.16.0",
    "@types/pg": "7.4.1",
    "axios": "0.16.2",
    "body-parser": "1.18.2",
    "config": "1.28.1",
    "cookie-parser": "1.4.3",
    "debug": "2.6.9",
    "express": "4.16.2",
    "express-engine": "1.0.1",
    "express-session": "1.15.6",
    "font-awesome-sass": "4.7.0",
    "foundation-sites": "6.4.3",
    "module-alias": "^2.0.3",
    "moment": "2.20.1",
    "morgan": "1.9.0",
    "node-cache": "4.1.1",
