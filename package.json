{
  "name": "tu_training_modules",
  "version": "0.0.1-SNAPSHOT",
  "description": "Tenants Union training modules application",
  "main": "app.js",
  "scripts": {
    "start": "./bin/start",
    "build": "npm run build-server && npm run build-webapp",
    "build-server": "tsc --project server/tsconfig.json --outDir server/dist",
    "build-webapp": "gulp -T build --gulpfile webapp/gulpfile.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Nodemon\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"nodemon dist/server.js\"",
    "watch-ts": "npm run tsc --watch --project server/tsconfig.json --outDir server/dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/egarasky/tu_training_modules.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/egarasky/tu_training_modules#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-engine": "^1.0.1",
    "glob": "^7.1.2",
    "hbs": "~4.0.1",
    "morgan": "~1.8.1",
    "q": "^1.5.0",
    "serve-favicon": "~2.4.2",
    "underscore": "^1.8.3",
    "vue": "^2.3.4",
    "vue-router": "^2.7.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.12",
    "@types/underscore": "^1.8.1",
    "browser-sync": "^2.18.12",
    "concurrently": "^3.5.0",
    "gulp-if": "^2.0.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-tap": "^1.0.1",
    "gulp-typescript": "^3.2.0",
    "nodemon": "^1.11.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript2": "^0.4.6",
    "rollup-stream": "^1.23.1",
    "typescript": "^2.4.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
