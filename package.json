    "@types/mocha": "2.2.43",
    "@types/node": "8.0.28",
    "@types/quill": "0.0.31",
    "@types/socket.io": "1.4.31",
    "@types/socket.io-client": "1.4.31",
    "@types/underscore": "1.8.3",
    "@types/winston": "2.3.5",
    "chai": "4.1.2",
    "concurrently": "3.5.0",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "loglevel": "1.5.0",
    "mocha": "3.5.3",
