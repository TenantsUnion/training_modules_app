{
  "name": "tu_training_modules",
  "version": "0.0.1-SNAPSHOT",
  "description": "Tenants Union training modules application",
  "main": "app.js",
  "scripts": {
    "start": "export SCRIPT_NAME=start && npm run esm_entry",
    "esm_entry": "export NODE_CONFIG_DIR=./config && node $NODE_DEBUG_OPTION ./bin/esm_script_runner",
    "init_db": "export SCRIPT_NAME=init_db && npm run esm_entry && npm run migrate_db",
    "old_init_db": "export NODE_CONFIG_DIR=./config && node ./bin/drop_db && node ./bin/init_db && npm run migrate_db",
    "drop_db": "export SCRIPT_NAME=drop_db && npm run esm_entry",
    "migrate_db": "export SCRIPT_NAME=migrate_db && npm run esm_entry",
    "build": "npm run server-build && npm run webpack-build",
    "webpack-build": "webpack --verbose --config webapp/webpack/production.webpack.config.js",
    "webpack-dev": "webpack-dev-server --config webapp/webpack/dev.webpack.config.js --open",
    "server-build": "tsc --project server/tsconfig.json --outDir server/dist",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Server-Build,Server,Webpack\" -c \"cyan.bold,green.bold,yellow.bold\"  \"npm run watch-server\" \"npm run nodemon-start\" \"npm run webpack-dev\"",
    "watch-server": "tsc --watch --project server/tsconfig.json --outDir server/dist",
    "nodemon-start": "export NODE_CONFIG_DIR=./config && nodemon --watch server/dist/ --signal SIGHUP --delay 1500ms --exec npm run start",
    "test_init_db": "export NODE_ENV=test && export SCRIPT_NAME=init_test_db && npm run test_migrate_db",
    "test_drop_db": "export NODE_ENV=test && npm run drop_db",
    "test_migrate_db": "export NODE_ENV=test && npm run migrate_db",
    "test": "export NODE_ENV=test export NODE_CONFIG_DIR=./config && npm run test_init_db && mocha $NODE_DEBUG_OPTION test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/egarasky/tu_training_modules.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/egarasky/tu_training_modules#readme",
  "@std/esm": {
    "esm": "js",
    "await": true,
    "cjs": true
  },
  "dependencies": {
    "@std/esm": "0.16.0",
    "@types/pg": "7.1.5",
    "axios": "0.16.2",
    "body-parser": "1.18.2",
    "config": "1.26.2",
    "cookie-parser": "1.4.3",
    "debug": "2.6.9",
    "express": "4.16.2",
    "express-engine": "1.0.1",
    "express-session": "1.15.6",
    "font-awesome-sass": "4.7.0",
    "foundation-sites": "6.4.3",
    "hbs": "4.0.1",
    "moment": "2.19.1",
    "morgan": "1.9.0",
    "node-cache": "4.1.1",
    "pg": "7.3.0",
    "promise": "8.0.1",
    "quill": "1.3.3",
    "quill-delta": "3.6.2",
    "serve-favicon": "2.4.5",
    "socket.io": "2.0.4",
    "socketio-wildcard": "2.0.0",
    "underscore": "1.8.3",
    "vue": "2.5.2",
    "vue-class-component": "6.0.0",
    "vue-form": "4.4.2",
    "vue-property-decorator": "6.0.0",
    "vue-router": "3.0.1",
