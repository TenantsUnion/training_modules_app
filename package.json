{
  "name": "tu_training_modules",
  "version": "0.0.1-SNAPSHOT",
  "description": "Tenants Union training modules application",
  "main": "app.js",
  "scripts": {
    "start": "export NODE_CONFIG_DIR=./config && node ./bin/start",
    "init_db": "export NODE_CONFIG_DIR=./config && node ./bin/drop_db && node ./bin/init_db && npm run migrate_db",
    "drop_db": "export NODE_CONFIG_DIR=./config && node ./bin/drop_db",
    "migrate_db": "export NODE_CONFIG_DIR=./config && node ./bin/migrate_db.js",
    "build": "npm run server-build && npm run webpack-build",
    "webpack-build": "webpack --config webapp/webpack/production.webpack.config.js",
    "webpack-dev": "webpack-dev-server --config webapp/webpack/dev.webpack.config.js --open",
    "server-build": "tsc --project server/tsconfig.json --outDir server/dist",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Server-Build,Server,Webpack\" -c \"cyan.bold,green.bold,yellow.bold\"  \"npm run watch-server\" \"npm run nodemon-start\" \"npm run webpack-dev\"",
    "watch-server": "tsc --watch --project server/tsconfig.json --outDir server/dist",
    "nodemon-start": "export NODE_CONFIG_DIR=./config && nodemon --watch server/dist/ ./bin/start",
    "test_init_db": "export NODE_ENV=test && NODE_CONFIG_DIR=./config node ./bin/init_test_db.js && npm run test_migrate_db",
    "test_migrate_db": "export NODE_ENV=test && node ./bin/migrate_db.js",
    "test": "export NODE_CONFIG_DIR=./config && mocha test/**/*.ts",
    "test-compile": "tsc --project test/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/egarasky/tu_training_modules.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/egarasky/tu_training_modules#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "config": "^1.26.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "^4.15.4",
    "express-engine": "^1.0.1",
    "express-session": "^1.15.5",
    "font-awesome-sass": "^4.7.0",
    "foundation-sites": "^6.4.3",
    "hbs": "~4.0.1",
    "lz-string": "^1.4.4",
    "moment": "^2.18.1",
    "morgan": "~1.8.1",
    "pg": "^7.3.0",
    "promise": "^8.0.1",
    "quill": "^1.3.2",
    "serve-favicon": "^2.4.4",
    "underscore": "^1.8.3",
    "vue": "^2.4.4",
    "vue-form": "^4.4.1",
    "vue-property-decorator": "^5.3.0",
    "vue-router": "^2.7.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/express-session": "^1.15.3",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.28",
    "@types/quill": "0.0.31",
    "@types/underscore": "^1.8.3",
    "@types/winston": "^2.3.5",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "loglevel": "^1.5.0",
    "mocha": "^3.5.3",
    "mocha-typescript": "^1.1.8",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "postcss-loader": "^2.0.6",
    "postgrator": "^2.10.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.0",
    "script-loader": "^0.7.1",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2",
    "uglify-es": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "vue-class-component": "^5.0.2",
    "vue-loader": "^12.2.2",
    "vue-template-compiler": "^2.4.4",
    "vue-ts-loader": "0.0.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">=8.0"
  }
}
