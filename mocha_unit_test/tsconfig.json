{
  "extends": "../base.tsconfig.json",
  "include": [
    "./src/**/*.ts",
    "../shared/*.ts",
    "../server/**/*.ts"
  ],
  "compilerOptions": {
    "lib": [
      "es5",
      "es2015.promise",
      "es6",
      "es7",
      "dom"
    ],
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@shared/*": [
        "../shared/*"
      ],
      "@server/*": [
        "../server/src/*"
      ],
      "@views/*": [
        "../server/src/views/*"
      ],
      "@v-course/*": [
        "../server/src/views/course/*"
      ],
      "@v-training/*": [
        "../server/src/views/training/*"
      ],
      "@v-user/*": [
        "../server/src/views/user/*"
      ],
      "@v-user_progress/*": [
        "../server/src/views/user_progress/*"
      ],
      "@handlers/*": [
        "../server/src/handlers/*"
      ],
      "@h-course/*": [
        "../server/src/handlers/course/*"
      ],
      "@h-module/*": [
        "../server/src/handlers/course/module/*"
      ],
      "@h-section/*": [
        "../server/src/handlers/course/module/section/*"
      ],
      "@h-training/*": [
        "../server/src/handlers/training/*"
      ],
      "@h-user_progress/*": [
        "../server/src/handlers/user_progress/*"
      ],
      "@mocha-root/*": [
        "./src/*"
      ],
      "@test-shared/*": [
        "../test_shared/*"
      ],
      "*": [
        "*"
      ]
    }
  }
}
