-- libraries should not be under version control
node_modules/

-- don't check in build
**/dist/**/*
**/build/**/*

-- ignore npm error logs
npm-debug.log

-- ignore vim backups
*~

-- ignore intellij IDE files
.idea/
*.iml

-- ignore generated in place .js files generated by IDE from compiling ts typescript files
-- Intellij keeps getting confused about just compiling and checking the ts files instead of generating the js files
test/**/*.js
test/**/*.js.map
webapp/src/app/**/*.js
webapp/src/app/**/*.js.map
server/src/**/*.js
server/src/**/*.js.map
